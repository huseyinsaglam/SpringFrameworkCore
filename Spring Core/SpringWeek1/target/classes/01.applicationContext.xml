<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="lassatiredid" class="_01.spring.model.Lassa"/>
    <bean id="goodyeartiredid" class="_01.spring.model.GoodYear"/>
    <bean id="petlastiredid" class="_01.spring.model.Petlas"/>
   
       

    <bean id="carid" class="_01.spring.model.Car">
       <property name="wheel" ref="lassatiredid"/>
    </bean>

    <!-- more bean definitions go here -->

</beans>